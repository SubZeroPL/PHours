pipeline {
    agent any
    stages {
        stage('SCM checkout') {
            steps {
                echo 'Checkout'
            }
        }
        stage('Build') {
            steps {
                echo 'Build'
            }
        }
        stage('Tests') {
            steps {
                echo 'Tests'
            }
        }
        stage('Publish artifacts') {
            steps {
                echo 'Publish'
            }
        }
    }
}